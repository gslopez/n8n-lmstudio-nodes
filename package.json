{
	"name": "n8n-nodes-lmstudio",
	"version": "0.1.1",
	"description": "N8N nodes for LM Studio integration with JSON schema support",
	"license": "MIT",
	"homepage": "https://github.com/gslopez/n8n-lmstudio-nodes",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"lm-studio",
		"ai",
		"llm"
	],
	"author": {
		"name": "Gonzalo Lopez",
		"email": "gslopez@example.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gslopez/n8n-lmstudio-nodes.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"test": "jest",
		"test:integration": "jest --config jest.integration.config.js",
		"prepublishOnly": "n8n-node prerelease",
		"prepare": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": false,
		"credentials": [
			"dist/credentials/LmStudioApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LmStudioSimpleMessage/LmStudioSimpleMessage.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/jest": "^30.0.0",
		"@types/node": "^22.0.0",
		"eslint": "9.32.0",
		"jest": "^30.2.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"ts-jest": "^29.4.6",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
